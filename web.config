<?xml version="1.0" encoding="utf-8"?>  
<configuration>  
    <system.webServer>           
      <handlers>  
           <add name="iisnode" path="server/server.js" verb="*" modules="iisnode"/>  
     </handlers>  
   </system.webServer>
    <rewrite>
  <rules>
    <rule name="website">
      <match url="/*" />
      <action type="Rewrite" url="server/server.js" />
    </rule>
  </rules>
</rewrite>
 </configuration>